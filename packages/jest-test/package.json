{
  "name": "@otel-test-runner/jest-test",
  "module": "dist/register.js",
  "types": "dist/register.d.ts",
  "main": "dist/register.js",
  "type": "module",
  "author": "TomChv",
  "license": "MIT",
  "version": "0.0.12",
  "description": "Jest Test Runner with Open telemetry support for auto-instrumentation with dagger",
  "scripts": {
    "build": "bun run build:register && bun run build:declaration",
    "build:register": "bun build --target=node --external @jest/globals ./register.ts --outfile=dist/register.js",
    "build:declaration": "tsc --emitDeclarationOnly"
  },
  "exports": {
    "./register": "./dist/register.js"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5",
    "@jest/globals": "^30.2.0"
  },
  "dependencies": {
    "@otel-test-runner/instrumentation": "0.0.12",
    "jest": "^30.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TomChv/ts-test-tracer/tree/main/packages/jest-test"
  },
  "keywords": [
    "mocha",
    "test",
    "dagger",
    "opentelemetry"
  ],
  "files": [
    "dist/*.js",
    "dist/**/*.d.ts",
    "README.md",
    "assets/*"
  ],
  "bugs": {
    "url": "https://github.com/TomChv/ts-test-tracer/issues"
  },
  "homepage": "https://github.com/TomChv/ts-test-tracer/tree/main#readme"
}
