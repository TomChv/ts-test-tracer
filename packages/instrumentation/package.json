{
  "name": "@otel-test-runner/instrumentation",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "author": "TomChv",
  "license": "MIT",
  "version": "0.0.8",
  "description": "A utility package to setup OpenTelemetry on Typescript test runners",
