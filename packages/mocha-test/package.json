{
  "name": "@otel-test-runner/mocha-test",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "type": "module",
  "author": "TomChv",
  "license": "MIT",
  "version": "0.1.0",
  "description": "Mocha Test Runner with Open telemetry support for auto-instrumentation with dagger",
  "scripts": {
    "build": "bun build --target=node ./index.ts --outfile=dist/index.js && bun run build:declaration",
    "build:declaration": "tsc --emitDeclarationOnly"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@types/bun": "latest",
    "@types/mocha": "^10.0.10"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@otel-test-runner/instrumentation": "0.1.0",
    "mocha": "^11.7.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TomChv/ts-test-tracer/tree/main/packages/mocha-test"
  },
  "keywords": [
    "mocha",
    "test",
    "dagger",
    "opentelemetry"
  ],
  "files": [
    "dist/*.js",
    "dist/**/*.d.ts",
    "README.md",
    "assets/*"
  ],
  "bugs": {
    "url": "https://github.com/TomChv/ts-test-tracer/issues"
  },
  "homepage": "https://github.com/TomChv/ts-test-tracer/tree/main#readme"
}
